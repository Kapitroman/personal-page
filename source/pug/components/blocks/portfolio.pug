mixin portfolio()
  section.portfolio.section-navigation#portfolio
    .container
      h2.titles(data-lang-key="headersSection")
      .portfolio__wrapper
        .portfolio__group
          .portfolio__group-images
            .portfolio__smartphone-image
              +picture-media({
                path: "content",
                image: "smartphone",
                format: "png",
                alt: "mockup smartphone",
                loading: true
              })
              .portfolio__smartphone-screen.portfolio__scroll
                +picture-media({
                  path: "content/portfolio",
                  image: "project-screen-mobile-002",
                  format: "jpg",
                  alt: "screen project",
                  loading: true
                })
            .portfolio__laptop-image
              +picture-media({
                media: "max-width: 767px",
                path: "content",
                image: "laptop",
                image2: "low-quality-3x3",
                format: "png",
                alt: "mockup laptop",
                loading: true
              })
              .portfolio__laptop-screen.portfolio__scroll
                +picture-media({
                  media: "max-width: 767px",
                  path: "content/portfolio",
                  image: "project-screen-laptop-002",
                  image2: "low-quality-3x3",
                  format: "jpg",
                  alt: "screen laptop",
                  loading: true
                })
          .portfolio__group-info
            .portfolio__group-links
              a.portfolio__link(href="https://kapitroman.github.io/severianochka/")
                +icon("icon-hyperlink", 18, 18)
                span(data-lang-key="linkLabels")
              a.portfolio__link(href="https://github.com/Kapitroman/severianochka")
                +icon("icon-github", 18, 18)
                span(data-lang-key="codeLabels")
            .portfolio__group-text
              p.portfolio__title(data-lang-key="projectNames")
              p.portfolio__description(data-lang-key="projectDescription")

        .portfolio__group
